{
  "name": "components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/components/components.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --watch --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "test.e2e": "stencil test --e2e",
    "test.e2e.watch": "stencil test --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -s dist -p 8082 --ci",
    "story": "npm-run-all --parallel build storybook"
  },
  "dependencies": {
    "@stencil/core": "2.4.0",
    "puppeteer": "^7.1.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "@stencil/postcss": "^2.0.0",
    "@stencil/sass": "^1.4.1",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-viewport": "^6.1.20",
    "@storybook/addons": "^6.2.8",
    "@storybook/html": "^6.1.21",
    "@storybook/react": "^6.1.20",
    "@storybook/theming": "^6.2.8",
    "@storybook/web-components": "^6.1.20",
    "@types/autoprefixer": "^10.2.0",
    "@types/jest": "^26.0.20",
    "@types/puppeteer": "^5.4.2",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "case": "^1.6.3",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lit-html": "^1.3.0",
    "npm-run-all": "^4.1.5"
  }
}
